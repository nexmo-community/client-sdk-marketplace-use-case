{
  "name": "client-sdk-marketplace",
  "version": "1.0.0",
  "description": "sample project for a Client SDK Marketplace Use Case",
  "main": "server.js",
  "scripts": {
    "client": "cd client && npm start",
    "server": "node server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "install": "cd ./client && npm install && cd ./"
  },
  "keywords": [],
  "author": "Dwane Hemmings",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.4",
    "nexmo": "^2.5.4-beta-1",
    "body-parser": "^1.19.0",
    "dotenv": "^6.2.0",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "concurrently": "^5.0.0"
  },
  "engines": {
    "node": "8.x"
  },
  "repository": {
    "url": "https://glitch.com/edit/#!/hello-express"
  }
}
